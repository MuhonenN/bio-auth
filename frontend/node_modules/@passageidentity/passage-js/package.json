{
  "name": "@passageidentity/passage-js",
  "version": "2.0.3",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint 'src/**/*.ts?(x)'",
    "format": "pretty-quick --staged",
    "prepare": "husky install",
    "build": "tsc --noEmit & tsup ./src/index.ts --format esm,cjs --dts --minify",
    "build:local": "echo 'export default {URL: \"http://auth.passage.local\"};' > ./src/config/env_config.ts && npm run build",
    "build:be_services_local": "echo 'export default {URL: \"http://localhost:3004\"};' > ./src/config/env_config.ts && npm run build",
    "build:dev": "echo 'export default {URL: \"https://auth.passage.dev\"};' > ./src/config/env_config.ts && npm run build",
    "build:uat": "echo 'export default {URL: \"https://auth-uat.passage.dev\"};' > ./src/config/env_config.ts && npm run build",
    "build:prod": "echo 'export default {URL: \"https://auth.passage.id\"};' > ./src/config/env_config.ts && npm run build",
    "docs": "typedoc",
    "change": "beachball change",
    "publish": "beachball publish -y -n",
    "beachball-check": "beachball check"
  },
  "keywords": [],
  "author": "Passage Identity, Inc.",
  "license": "ISC",
  "devDependencies": {
    "@types/base-64": "^1.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "beachball": "^2.20.0",
    "eslint": "^8.5.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.2",
    "tsup": "^5.11.11",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.10.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/node": "^15.0.2",
    "jwt-decode": "^3.1.2"
  }
}
